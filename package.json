    "url": "git://github.com:ZauberNerd/small-eventemitter.git"
  },
  "bugs": {
    "url": "https://github.com/ZauberNerd/small-eventemitter/issues"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.2",
    "jscs": "~1.7.3",
    "jshint": "~2.5.6",
    "tape": "~3.0.1"
  },
  "keywords": [
    "eventemitter",
    "emitter",
    "event",
    "events",
    "mediator"
