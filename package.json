{
  "name": "small-eventemitter",
  "version": "1.0.1",
  "license": "MIT",
  "description": "A very small EventEmitter implementation for client- and server-side code.",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run cover && npm run jshint && npm run jscs",
    "test-browser": "zuul -- test/*.js",
    "jshint": "jshint -c .jshintrc lib/index.js",
    "jscs": "jscs --config .jscsrc lib/index.js",
    "cover": "istanbul cover tape test/*.js",
    "coveralls": "cat coverage/lcov.info | coveralls"
  },
  "author": {
    "name": "ZauberNerd",
    "email": "zaubernerd@zaubernerd.de"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com:ZauberNerd/small-eventemitter.git"
  },
  "bugs": {
    "url": "https://github.com/ZauberNerd/small-eventemitter/issues"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.2",
    "jscs": "~1.7.3",
    "jshint": "~2.5.6",
    "tape": "~3.0.1",
    "zuul": "^1.13.0"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  },
  "keywords": [
    "eventemitter",
    "emitter",
    "event",
    "events",
    "mediator"
  ]
}
